#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x555cecdda5e0 .scope module, "Data_Memory_tb" "Data_Memory_tb" 2 3;
 .timescale -9 -12;
v0x555cecdf3c00_0 .var "MemRead", 0 0;
v0x555cecdf3cc0_0 .var "MemWrite", 0 0;
v0x555cecdf3d90_0 .var "address", 63 0;
v0x555cecdf3e90_0 .var "clk", 0 0;
v0x555cecdf3f60_0 .net "read_data", 63 0, v0x555cecdf3980_0;  1 drivers
v0x555cecdf4050_0 .var "write_data", 63 0;
S_0x555cecdda770 .scope module, "uut" "Data_Memory" 2 14, 3 4 0, S_0x555cecdda5e0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "MemRead";
    .port_info 2 /INPUT 1 "MemWrite";
    .port_info 3 /INPUT 64 "address";
    .port_info 4 /INPUT 64 "write_data";
    .port_info 5 /OUTPUT 64 "read_data";
v0x555cecdda920_0 .net "MemRead", 0 0, v0x555cecdf3c00_0;  1 drivers
v0x555cecdf3630_0 .net "MemWrite", 0 0, v0x555cecdf3cc0_0;  1 drivers
v0x555cecdf36f0_0 .net "address", 63 0, v0x555cecdf3d90_0;  1 drivers
v0x555cecdf37b0_0 .net "clk", 0 0, v0x555cecdf3e90_0;  1 drivers
v0x555cecdf3870 .array "memory", 255 0, 63 0;
v0x555cecdf3980_0 .var "read_data", 63 0;
v0x555cecdf3a60_0 .net "write_data", 63 0, v0x555cecdf4050_0;  1 drivers
E_0x555cecdd6760 .event negedge, v0x555cecdf37b0_0;
    .scope S_0x555cecdda770;
T_0 ;
    %pushi/vec4 0, 0, 64;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x555cecdf3870, 4, 0;
    %pushi/vec4 1, 0, 64;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x555cecdf3870, 4, 0;
    %pushi/vec4 2, 0, 64;
    %ix/load 4, 2, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x555cecdf3870, 4, 0;
    %pushi/vec4 3, 0, 64;
    %ix/load 4, 3, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x555cecdf3870, 4, 0;
    %pushi/vec4 4, 0, 64;
    %ix/load 4, 4, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x555cecdf3870, 4, 0;
    %pushi/vec4 5, 0, 64;
    %ix/load 4, 5, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x555cecdf3870, 4, 0;
    %pushi/vec4 6, 0, 64;
    %ix/load 4, 6, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x555cecdf3870, 4, 0;
    %pushi/vec4 7, 0, 64;
    %ix/load 4, 7, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x555cecdf3870, 4, 0;
    %pushi/vec4 8, 0, 64;
    %ix/load 4, 8, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x555cecdf3870, 4, 0;
    %end;
    .thread T_0;
    .scope S_0x555cecdda770;
T_1 ;
    %wait E_0x555cecdd6760;
    %load/vec4 v0x555cecdf3630_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %load/vec4 v0x555cecdf3a60_0;
    %load/vec4 v0x555cecdf36f0_0;
    %parti/s 10, 0, 2;
    %ix/vec4 3;
    %ix/load 4, 0, 0; Constant delay
    %assign/vec4/a/d v0x555cecdf3870, 0, 4;
T_1.0 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x555cecdda770;
T_2 ;
    %wait E_0x555cecdd6760;
    %load/vec4 v0x555cecdda920_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.0, 8;
    %load/vec4 v0x555cecdf36f0_0;
    %parti/s 10, 0, 2;
    %ix/vec4 4;
    %load/vec4a v0x555cecdf3870, 4;
    %assign/vec4 v0x555cecdf3980_0, 0;
T_2.0 ;
    %jmp T_2;
    .thread T_2;
    .scope S_0x555cecdda5e0;
T_3 ;
    %delay 5000, 0;
    %load/vec4 v0x555cecdf3e90_0;
    %inv;
    %store/vec4 v0x555cecdf3e90_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x555cecdda5e0;
T_4 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x555cecdf3e90_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x555cecdf3c00_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x555cecdf3cc0_0, 0, 1;
    %pushi/vec4 0, 0, 64;
    %store/vec4 v0x555cecdf3d90_0, 0, 64;
    %pushi/vec4 0, 0, 64;
    %store/vec4 v0x555cecdf4050_0, 0, 64;
    %delay 10000, 0;
    %delay 10000, 0;
    %pushi/vec4 3, 0, 64;
    %store/vec4 v0x555cecdf3d90_0, 0, 64;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x555cecdf3c00_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x555cecdf3c00_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 3, 0, 64;
    %store/vec4 v0x555cecdf3d90_0, 0, 64;
    %pushi/vec4 3735928559, 0, 32;
    %concati/vec4 3405691582, 0, 32;
    %store/vec4 v0x555cecdf4050_0, 0, 64;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x555cecdf3cc0_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x555cecdf3cc0_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x555cecdf3c00_0, 0, 1;
    %pushi/vec4 3, 0, 64;
    %store/vec4 v0x555cecdf3d90_0, 0, 64;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x555cecdf3c00_0, 0, 1;
    %delay 20000, 0;
    %vpi_call 2 80 "$finish" {0 0 0};
    %end;
    .thread T_4;
    .scope S_0x555cecdda5e0;
T_5 ;
    %vpi_call 2 85 "$monitor", "Time=%0t | clk=%b | MemRead=%b | MemWrite=%b | Addr=%d | WriteData=%h | ReadData=%h", $time, v0x555cecdf3e90_0, v0x555cecdf3c00_0, v0x555cecdf3cc0_0, v0x555cecdf3d90_0, v0x555cecdf4050_0, v0x555cecdf3f60_0 {0 0 0};
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "Data_Memory_tb.v";
    "Data_Memory.v";
